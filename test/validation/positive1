types
    loc dfrac val

preds
    valid : dfrac -> Prop
    pointsto : loc * dfrac * val -> iProp

consts
    Own : dfrac
    Discarded : dfrac
    l : loc
    v : val

facts
    Persistent (pointsto l Discarded v),

laws
    pointsto l Own v -∗ valid Own,
    l ↦{dq1} v1 -∗ l ↦{dq2} v2 -∗ ⌜✓ (dq1 ⋅ dq2) ∧ v1 = v2⌝.
